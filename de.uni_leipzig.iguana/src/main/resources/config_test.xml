<!-- 			<percent value="0.1" file-name="dbpedia2/"/> -->
<!-- 			<percent value="2.0" file-name="dbpedia2/"/> -->
		</random-function>
		<warmup time="0" file-name="warmup.txt"/>
		<test-db type="choose" reference="ref">
<!-- 			<db id="owlim" /> -->
			<db id="blazegraph" />
			<db id="virtuoso_4" />
<!-- 			<db id="fuseki" />-->
