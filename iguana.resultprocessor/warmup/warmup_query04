
sparql select ?title ?group count(?post) as ?cnt #Q5
where { 
    {select distinct ?fr
     where {
       {sn:pers00000021990232560330 snvoc:knows ?fr.} union {sn:pers00000021990232560330 snvoc:knows ?fr2. ?fr2 snvoc:knows ?fr. filter (?fr != sn:pers00000021990232560330)}
     }
    } .
    ?group snvoc:title ?title. ?group snvoc:hasMember ?mem .
    ?mem snvoc:hasPerson ?fr . ?mem snvoc:joinDate ?date .
    filter (?date >= "2012-09-21T02:00:00.000+00:00"^^xsd:dateTime) .
    optional {
      ?post snvoc:hasCreator ?fr .
      ?group snvoc:containerOf ?post .
    } .
}
group by ?title ?group
order by desc(?cnt) ?group
limit 20
;

