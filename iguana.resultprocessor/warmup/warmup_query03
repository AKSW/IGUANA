
sparql select ?tagname count (*) as ?cnt #Q4
where {
    ?post a snvoc:Post . ?post snvoc:hasCreator ?fr .
    ?post snvoc:hasTag ?tag . ?tag foaf:name ?tagname .
    ?post snvoc:creationDate ?date . sn:pers00000017592186047257 snvoc:knows ?fr .
    filter (?date >= "2010-12-01T01:00:00.000+00:00"^^xsd:dateTime && ?date <= "2010-12-01T01:00:00.000+00:00"^^xsd:dateTime + xsd:duration("P171D")) .
    filter (!exists {
        sn:pers00000017592186047257 snvoc:knows ?fr2 . ?post2 snvoc:hasCreator ?fr2 .
	?post2 a snvoc:Post . ?post2 snvoc:hasTag ?tag .
        ?post2 snvoc:creationDate ?date2 .
	filter (?date2 < "2010-12-01T01:00:00.000+00:00"^^xsd:dateTime)}) 
    }
group by ?tagname
order by desc(?cnt) ?tagname
limit 10
;

